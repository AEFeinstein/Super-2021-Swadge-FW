{
    "configurations": [
        {
            "name": "ESP8266-GCC",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/user",
                "${workspaceFolder}/user/display",
                "${workspaceFolder}/user/display/fonts",
                "${workspaceFolder}/user/modes",
                "${workspaceFolder}/user/modes/colorchord",
                "${workspaceFolder}/user/modes/personal_demon",
                "${workspaceFolder}/user/modes/screensavers",
                "${workspaceFolder}/user/hdw",
                "${workspaceFolder}/user/utils",
                "${workspaceFolder}/user/utils/spi_mem",
                "${workspaceFolder}/user/utils/wireless",
                "${workspaceFolder}/emu",
                "${workspaceFolder}/emu/rawdraw",
                "${workspaceFolder}/emu/sound",
                "${workspaceFolder}/emu/sysincstubs",
                "${workspaceFolder}/emu/sysincstubs/driver",
                "${workspaceFolder}/ESP8266_NONOS_SDK/include",
                "${workspaceFolder}/ESP8266_NONOS_SDK/driver_lib/include/",
                "${workspaceFolder}/ESP8266_NONOS_SDK/driver_lib/include/driver/",
                "${env:ESP_ROOT}/xtensa-lx106-elf/include"
            ],
            "defines": [
                "USE_OPTIMIZE_PRINTF=1",
                "ICACHE_FLASH=1",
                "SOFTAP_CHANNEL=11",
                "WS2812_FOUR_SAMPLE=1",
                "SWADGE_VERSION=5",
                "FW_FILE1_ADDR=0x00000",
                "FW_FILE1_SIZE=0x10000",
                "FW_FILE2_ADDR=0x10000",
                "FW_FILE2_SIZE=0x6D000",
                "USER_SETTINGS_ADDR=0x7D000",
                "USER_SETTINGS_SIZE=0x3000",
                "ASSETS_ADDR=0x80000",
                "ASSETS_SIZE=0x40000",
                "RF_CAL_ADDR=0x1FB000",
                "RF_CAL_SIZE=0x1000",
                "PHY_DATA_ADDR=0x1FC000",
                "PHY_DATA_SIZE=0x1000",
                "SYS_PARAM_ADDR=0x1FD000",
                "SYS_PARAM_SIZE=0x3000",
                "GIT_HASH=\"tmp-git-hash\""
            ],
            "compilerPath": "${env:ESP_ROOT}/xtensa-lx106-elf/bin/xtensa-lx106-elf-gcc",
            "cStandard": "gnu99",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}